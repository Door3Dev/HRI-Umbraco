      <IndividualPlans id="1234" parentID="1128" level="3" creatorID="1" sortOrder="0" createDate="2014-06-21T00:18:46" updateDate="2014-09-24T15:54:11" nodeName="Individual Plans" urlName="individual-plans" path="-1,1124,1128,1234" isDoc="" nodeType="1232" creatorName="door3" writerName="kthomas" writerID="3" template="1240" nodeTypeAlias="IndividualPlans">
