    <NavigationCategory id="1131" parentID="1124" level="2" creatorID="1" sortOrder="3" createDate="2014-06-12T11:22:14" updateDate="2014-08-22T16:03:48" nodeName="For Providers" urlName="for-providers" path="-1,1124,1131" isDoc="" nodeType="1125" creatorName="door3" writerName="door3" writerID="1" template="0" nodeTypeAlias="NavigationCategory">
