@using Umbraco.Web.Models
@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "MasterPage.cshtml";
}
<div id="hri-content" class="hri-login">
        <div class="row-fluid">

          <div class="span4 offset4 well">

            <h3>Member Login</h3>

            @{Html.RenderPartial("LoginForm", Model.Content);}

            Enrolled in a Health Republic Plan? <a href="/user/register">Register for an account.</a>
            <br><br>
            Forgot <a href="~/for-members/forgot-username">Username?</a> <span>|</span>
            Forgot <a href="~/for-members/forgot-password">Password?</a><br>
            Resend <a href="~/for-members/resend-email">Email Verification Link</a>
            <br><br>
            If you are having trouble logging in or registering, call us at <b>888.990.5702</b>.

          </div>
        </div>
      </div>